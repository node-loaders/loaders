{
  "name": "@node-loaders/esbuildx",
  "version": "0.2.2",
  "keywords": [
    "esm",
    "tsnode",
    "tsx",
    "tsm",
    "loader",
    "node",
    "esbuild",
    "typescript",
    "esm-loader"
  ],
  "bugs": "https://github.com/node-loaders/loaders/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/node-loaders/loaders.git"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "esbuildx": "./dist/bin.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/esbuildx.d.ts",
      "default": "./dist/esbuildx.mjs"
    }
  },
  "files": [
    "./dist"
  ],
  "types": "./dist/esbuildx.d.ts",
  "scripts": {
    "build": "tsc",
    "postbuild": "cpy bin.mjs ../dist --cwd=src",
    "clean": "rimraf dist",
    "clean-all": "npm run clean && rimraf node_modules",
    "fix": "xo --fix",
    "pretest": "xo",
    "test": "./dist/bin.mjs ./test/cts.cts && ./dist/bin.mjs ./test/mts.mjs && ./dist/bin.mjs ./test/ts.ts",
    "test-coverage": "npm run test"
  },
  "dependencies": {
    "@node-loaders/esbuild": "^0.7.1",
    "cross-spawn": "^7.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "cpy-cli": "^4.2.0"
  }
}
